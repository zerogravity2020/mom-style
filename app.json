{
  "expo": {
    "name": "Mom Style",
    "slug": "mom-app",
    "version": "1.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.1",
      "bundleIdentifier": "com.momstyle.momstyle"
    },
    "androidStatusBar": {
      "translucent": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 3,
      "package": "com.momstyle.momstyle",
      "useNextNotificationsApi": true
    },
    "notification": {
      "icon": "./assets/notificatoin-icon.png"
    }
  }
}
